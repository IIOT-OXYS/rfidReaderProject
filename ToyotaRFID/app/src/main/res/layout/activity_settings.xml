<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nzar.toyotarfid.SettingsActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="200dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/db_url"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/toyota_template"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/db_pw"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Database Password"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/db_engine"
        app:layout_constraintRight_toRightOf="@+id/db_port"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="13dp"
        app:layout_constraintLeft_toLeftOf="@+id/db_port"
        app:layout_constraintHorizontal_bias="0.0" />

    <EditText
        android:id="@+id/db_port"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Port #"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/db_pw"
        android:layout_marginStart="613dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="613dp"
        android:layout_marginTop="16dp" />



    <EditText
        android:id="@+id/db_url"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Database IP/Domain"
        android:inputType="textPersonName"
        android:textSize="30sp"
        app:layout_constraintRight_toLeftOf="@+id/db_port"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/db_user"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="15dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <EditText
        android:id="@+id/wifi_ssid"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Wireless SSID"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/static_ip"
        app:layout_constraintRight_toRightOf="@+id/db_name"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/db_name" />

    <EditText
        android:id="@+id/db_user"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Database Username"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/db_name"
        app:layout_constraintRight_toRightOf="@+id/db_url"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="13dp"
        app:layout_constraintLeft_toLeftOf="@+id/db_url" />

    <EditText
        android:id="@+id/db_engine"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Database Engine"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/wifi_pw"
        app:layout_constraintRight_toRightOf="@+id/db_pw"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="-13dp"
        app:layout_constraintLeft_toLeftOf="@+id/db_pw"
        app:layout_constraintHorizontal_bias="0.0" />

    <EditText
        android:id="@+id/wifi_pw"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Wireless Password"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/netmask"
        app:layout_constraintRight_toRightOf="@+id/db_engine"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/db_engine" />

    <EditText
        android:id="@+id/netmask"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Subnet mask"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/wifi_pw"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="4dp"
        app:layout_constraintLeft_toLeftOf="@+id/wifi_pw" />

    <EditText
        android:id="@+id/db_name"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:ems="10"
        android:hint="Database Name"
        android:inputType="textPersonName"
        android:textSize="30sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/wifi_ssid"
        app:layout_constraintRight_toRightOf="@+id/db_user"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="1dp"
        app:layout_constraintLeft_toLeftOf="@+id/db_user" />

    <EditText
        android:id="@+id/static_ip"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ems="10"
        android:hint="Static IP"
        android:inputType="text|textPersonName"
        android:textSize="30sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/wifi_ssid"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="3dp"
        app:layout_constraintLeft_toLeftOf="@+id/wifi_ssid"
        app:layout_constraintTop_toTopOf="@+id/netmask" />





    <Button
        android:id="@+id/set_button"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginStart="16dp"
        android:background="@android:color/holo_green_dark"
        android:text="Set"
        android:textSize="30sp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/back_button"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="46dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <Button
        android:id="@+id/back_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Back"
        android:textSize="30sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/set_button"
        android:layout_marginTop="30dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/set_button"
        app:layout_constraintTop_toTopOf="@+id/wifi_ssid" />

</android.support.constraint.ConstraintLayout>
