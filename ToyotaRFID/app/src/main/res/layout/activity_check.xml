<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nzar.toyotarfid.CheckActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp"
    android:id="@+id/check_activity_root_layout">


    <Button
        android:id="@+id/CheckActivityCancelButton"
        android:layout_width="238dp"
        android:layout_height="112dp"
        android:background="#434343"
        android:text="@string/cancel"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="MissingConstraints,RtlHardcoded" />

    <Button
        android:id="@+id/Yes"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="238dp"
        android:layout_height="112dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:background="#659941"
        android:text="@string/y"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.812"
        app:layout_constraintLeft_toRightOf="@+id/CheckActivityCancelButton"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        tools:ignore="MissingConstraints,RtlHardcoded" />

    <Button
        android:id="@+id/Contact"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="238dp"
        android:layout_height="112dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="24dp"
        android:background="#E55125"
        android:filterTouchesWhenObscured="false"
        android:text="@string/tech"
        android:textSize="36sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintRight_toLeftOf="@+id/textClock"
        tools:ignore="MissingConstraints,RtlHardcoded"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="24dp" />


    <TextClock
        android:id="@+id/textClock"
        android:layout_width="238dp"
        android:layout_height="112dp"
        android:background="#D0D2D3"
        android:fontFamily="sans-serif"
        android:paddingTop="10dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#434343"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/PPE1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:elevation="0dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/PPE6"
        app:layout_constraintTop_toBottomOf="@+id/PPERequirements" />

    <Button
        android:id="@+id/PPE2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:maxWidth="100dp"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintLeft_toRightOf="@+id/PPE1"
        app:layout_constraintRight_toLeftOf="@+id/PPE3"
        app:layout_constraintTop_toTopOf="@+id/PPE1" />

    <Button
        android:id="@+id/PPE3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/PPE2" />

    <Button
        android:id="@+id/PPE4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintLeft_toRightOf="@+id/PPE3"
        app:layout_constraintRight_toLeftOf="@+id/PPE5"
        app:layout_constraintTop_toTopOf="@+id/PPE3" />

    <Button
        android:id="@+id/PPE5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/PPE4" />

    <Button
        android:id="@+id/PPE6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="32dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/CheckActivityCancelButton"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/PPE7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintLeft_toRightOf="@+id/PPE6"
        app:layout_constraintRight_toLeftOf="@+id/PPE8"
        app:layout_constraintTop_toTopOf="@+id/PPE6" />

    <Button
        android:id="@+id/PPE8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/PPE7" />

    <Button
        android:id="@+id/PPE9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="9dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="9dp"
        android:layout_marginStart="8dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toRightOf="@+id/PPE8"
        app:layout_constraintRight_toLeftOf="@+id/PPE10"
        app:layout_constraintTop_toTopOf="@+id/PPE8" />

    <Button
        android:id="@+id/PPE10"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:maxWidth="100dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/PPE9" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tiltlab_banner"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/PPERequirements"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="To enable START, Tap to verify you meet the following PPE requirements of this device"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/PPE3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.476" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="300dp" />

    <TextView
        android:id="@+id/PPEText1"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE1"
        app:layout_constraintRight_toRightOf="@+id/PPE1"
        app:layout_constraintLeft_toLeftOf="@+id/PPE1"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/PPE6" />

    <TextView
        android:id="@+id/PPEText2"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="@+id/PPE2"
        app:layout_constraintRight_toRightOf="@+id/PPE2"
        app:layout_constraintTop_toBottomOf="@+id/PPE2"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/PPE7" />

    <TextView
        android:id="@+id/PPEText3"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintRight_toRightOf="@+id/PPE3"
        app:layout_constraintLeft_toLeftOf="@+id/PPE3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE3"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/PPE8" />

    <TextView
        android:id="@+id/PPEText5"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/PPE5"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE5"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/PPE10"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/PPE5" />

    <TextView
        android:id="@+id/PPEText4"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/PPE4"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/PPE4"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE4"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/PPE9"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/PPEText6"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintRight_toRightOf="@+id/PPE6"
        app:layout_constraintLeft_toLeftOf="@+id/PPE6"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE6"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/CheckActivityCancelButton" />

    <TextView
        android:id="@+id/PPEText7"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/Yes"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE7"
        app:layout_constraintRight_toRightOf="@+id/PPE7"
        app:layout_constraintLeft_toLeftOf="@+id/PPE7" />

    <TextView
        android:id="@+id/PPEText9"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintRight_toRightOf="@+id/PPE9"
        app:layout_constraintLeft_toLeftOf="@+id/PPE9"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE9"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/Contact" />

    <TextView
        android:id="@+id/PPEText8"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/PPE8"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/PPE8"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE8"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/Yes"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/PPEText10"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintRight_toRightOf="@+id/PPE10"
        app:layout_constraintLeft_toLeftOf="@+id/PPE10"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/PPE10"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/textClock" />

    <View
        android:layout_width="200dp"
        android:layout_height="2dp"
        android:background="#434343"
        app:layout_constraintTop_toTopOf="@+id/textClock"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textClock"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/textClock"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/textClock"
        android:id="@+id/view"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/TextDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#434343"
        android:textSize="24sp"
        android:text="Sep 24, 1997"
        app:layout_constraintBottom_toBottomOf="@+id/textClock"
        android:layout_marginBottom="24dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/textClock"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/textClock"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

</android.support.constraint.ConstraintLayout>

